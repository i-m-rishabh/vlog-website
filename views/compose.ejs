<%- include("partials/header") -%>

<h1>compose</h1>
<form class="form-group" action="/compose" method="post">
        
        <div class="form-group">
          <label for="postTitle">Title</label>
          <input type="text" class="form-control" name="postTitle" id="postTitle" placeholder="" autocomplete="off">
        </div>

        <div class="form-group">
          <label for="post">Post</label>
          <textarea class="form-control" name="postBody" id="post" rows="5"></textarea>
        </div>
    
    <button type="submit" name="button" id="" class="btn btn-primary ">Publish</button>
</form>



<%- include("partials/footer") -%>